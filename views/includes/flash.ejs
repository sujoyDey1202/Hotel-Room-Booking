<% if(success && success.length){ %>
    <div class="alert alert-success alert-dismissble fade show col-6 offset-3" role="alert">
         <%= success %>
         <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
<%}%>
<% if(error && error.length){ %>
    <div class="alert alert-danger alert-dismissble fade show col-6 offset-3" role="alert">
         <%= error %>
         <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
<%}%>